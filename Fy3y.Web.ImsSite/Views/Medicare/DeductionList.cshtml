@model IList<Fy3y.Model.Das.MedicareDeduction>

@{
    ViewData["Title"] = "医保扣款查询";
}

<form method="post">
    @Html.AntiForgeryToken()
    <fieldset>
        <legend>查询条件</legend>
        <div class="form-group row">
            <label class="col-form-label col-2 text-right">选择月份</label>
            <input type="text" class="form-control col-2" name="month" value="@ViewBag.month" onfocus="WdatePicker({ isShowClear: false, readOnly: true, isShowOK: false, dateFmt: 'yyyyMM' })" />
            <div class="col-2"><button class="btn btn-success"><i class="fa fa-search"></i>查询</button></div>

        </div>
    </fieldset>
</form>
<br />
<table class="table table-bordered table-sm">
    <thead class="text-center">
        <tr>
            <th>#</th>
            <th>
                @Html.DisplayNameFor(model => model.Jyfs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CbrXm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ksmc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ysxm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Xmmc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Xmsl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lzje)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Kcje)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Kcyy)
            </th>
        </tr>
    </thead>
    <tbody>
        @{
            var i = 1;
            foreach (var item in Model)
            {
            <tr>
                <td>@i</td>
                <td>
                    @Html.DisplayFor(model => item.Jyfs)
                </td>
                <td>
                    @Html.DisplayFor(model => item.CbrXm)
                </td>
                <td>
                    @Html.DisplayFor(model => item.Ksmc)
                </td>
                <td>
                    @Html.DisplayFor(model => item.Ysxm)
                </td>
                <td>
                    @Html.DisplayFor(model => item.Xmmc)
                </td>
                <td>
                    @Html.DisplayFor(model => item.Xmsl)
                </td>
                <td>
                    @Html.DisplayFor(model => item.Lzje)
                </td>
                <td>
                    @Html.DisplayFor(model => item.Kcje)
                </td>
                <td>
                    @Html.DisplayFor(model => item.Kcyy)
                </td>
            </tr>
                i++;
            }
        }
    </tbody>
</table>
