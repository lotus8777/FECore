@model IList<Gy_Ygdm>
@{
  ViewBag.Title = "同步账户信息";
  Layout = "~/Views/Shared/_Layout.cshtml";
}
<table class="table table-sm table-bordered text-center table-striped">
  <thead>
    <tr>
      <th>#</th>
      <th>账号</th>
      <th>姓名</th>
      <th>操作</th>
    </tr>
  </thead>
  <tbody>
    @{
    if (Model?.Count() > 0)
    {
      var i = 1;
      foreach (var item in Model)
      {
      <tr>
        <td>@i </td>
        <td>@item.Ygdm </td>
        <td>@item.Ygxm </td>
        <td><span class="badge badge-success"> 同步成功</span> </td>
      </tr>
        i++;
      }
    }
    }
  </tbody>
</table>