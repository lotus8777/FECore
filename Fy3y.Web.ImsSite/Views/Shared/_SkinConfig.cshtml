<div class="theme-config">
  <div class="theme-config-box">
    <div class="spin-icon">
      <i class="fa fa-cogs fa-spin"></i>
    </div>
    <div class="skin-settings">
      <div class="title">Configuration</div>
      <div class="setings-item">
        <span>
          Collapse menu
        </span>
        <div class="switch">
          <div class="onoffswitch">
            <input class="onoffswitch-checkbox" id="collapsemenu" name="collapsemenu" type="checkbox">
            <label class="onoffswitch-label" for="collapsemenu">
              <span class="onoffswitch-inner"></span>
              <span class="onoffswitch-switch"></span>
            </label>
          </div>
        </div>
      </div>
      <div class="setings-item">
        <span>
          Fixed sidebar
        </span>
        <div class="switch">
          <div class="onoffswitch">
            <input class="onoffswitch-checkbox" id="fixedsidebar" name="fixedsidebar" type="checkbox">
            <label class="onoffswitch-label" for="fixedsidebar">
              <span class="onoffswitch-inner"></span>
              <span class="onoffswitch-switch"></span>
            </label>
          </div>
        </div>
      </div>
      <div class="setings-item">
        <span>
          Top navbar
        </span>
        <div class="switch">
          <div class="onoffswitch">
            <input class="onoffswitch-checkbox" id="fixednavbar" name="fixednavbar" type="checkbox">
            <label class="onoffswitch-label" for="fixednavbar">
              <span class="onoffswitch-inner"></span>
              <span class="onoffswitch-switch"></span>
            </label>
          </div>
        </div>
      </div>
      <div class="setings-item">
        <span>
          Top navbar v.2
          <br />
          <small>*Primary layout</small>
        </span>
        <div class="switch">
          <div class="onoffswitch">
            <input class="onoffswitch-checkbox" id="fixednavbar2" name="fixednavbar2" type="checkbox">
            <label class="onoffswitch-label" for="fixednavbar2">
              <span class="onoffswitch-inner"></span>
              <span class="onoffswitch-switch"></span>
            </label>
          </div>
        </div>
      </div>
      <div class="setings-item">
        <span>
          Boxed layout
        </span>
        <div class="switch">
          <div class="onoffswitch">
            <input class="onoffswitch-checkbox" id="boxedlayout" name="boxedlayout" type="checkbox">
            <label class="onoffswitch-label" for="boxedlayout">
              <span class="onoffswitch-inner"></span>
              <span class="onoffswitch-switch"></span>
            </label>
          </div>
        </div>
      </div>
      <div class="setings-item">
        <span>
          Fixed footer
        </span>
        <div class="switch">
          <div class="onoffswitch">
            <input class="onoffswitch-checkbox" id="fixedfooter" name="fixedfooter" type="checkbox">
            <label class="onoffswitch-label" for="fixedfooter">
              <span class="onoffswitch-inner"></span>
              <span class="onoffswitch-switch"></span>
            </label>
          </div>
        </div>
      </div>
      <div class="title">Skins</div>
      <div class="default-skin setings-item">
        <span class="skin-name ">
          <a class="s-skin-0" href="#">
            Default
          </a>
        </span>
      </div>
      <div class="blue-skin setings-item">
        <span class="skin-name ">
          <a class="s-skin-1" href="#">
            Blue light
          </a>
        </span>
      </div>
      <div class="setings-item yellow-skin">
        <span class="skin-name ">
          <a class="s-skin-3" href="#">
            Yellow/Purple
          </a>
        </span>
      </div>
      <div class="setings-item ultra-skin">
        <span class="skin-name ">
          <a class="md-skin" href="md_skin/" target="_blank">
            Material Design
          </a>
        </span>
      </div>
    </div>
  </div>
</div>