<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta content="text/html; charset=utf-8" http-equiv="Content-Type" />
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <title>@ViewBag.Title - DAS数据分析系统</title>

    <link rel="stylesheet" href="~/Scripts/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/Scripts/lib/dataTables/media/css/dataTables.bootstrap4.min.css" />
    <link rel="stylesheet" href="~/Scripts/lib/layer/theme/default/layer.css" />
    <link rel="stylesheet" href="~/Scripts/lib/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/Content/animate.css" />
    <link rel="stylesheet" href="~/Content/style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Content/print.css" />
    <link rel="stylesheet" href="~/Content/site.css" />
    <script src="~/Scripts/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/Scripts/lib/popper.js/dist/umd/popper.min.js"></script>
    <script src="~/Scripts/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/Scripts/lib/dataTables/media/js/jquery.dataTables.js"></script>
    <script src="~/Scripts/lib/dataTables/media/js/dataTables.bootstrap4.js"></script>
    <script src="~/Scripts/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/Scripts/lib/metisMenu/dist/jquery.metisMenu.js"></script>
    <script src="~/Scripts/lib/pace/pace.min.js"></script>
    <script src="~/Scripts/lib/slimScroll/jquery.slimscroll.min.js"></script>
    <script src="~/Scripts/script.js" asp-append-version="true"></script>
    <script src="~/Scripts/site.js"></script>
    <script src="~/Scripts/lib/My97DatePicker/WdatePicker.js"></script>
    <script src="~/Scripts/lib/layer/layer.js"></script>
    <script type="text/javascript">
        var agent = navigator.userAgent; //check if the browser is IE
        if (agent.indexOf("Firefox") === -1 &&
            agent.indexOf("Chrome") === -1 &&
            agent.indexOf("AppleWebKit") === -1 &&
            agent.indexOf("Safari") === -1) {
            window.location.href = '@Url.Action("LowVersion","Home")';
        }
    </script>
    <!--打印样式-->
</head>
<body>
<a class="display-4 d-print-none" href="@Url.Action("Index","Prevalence")">疫情中高风险区</a>
<div class="text-center" id="pageHeader">
    <h2>@ViewBag.Title</h2>
    @if (ViewBag.SubTitle != null)
    {
        @ViewBag.SubTitle
    }
</div>
<div style="margin: 30px;">
    @RenderBody()
</div>

</body>
</html>
<script type="text/javascript">
    jqueryDataTable("table1");
    jqueryFullTable("tableFull");
</script>