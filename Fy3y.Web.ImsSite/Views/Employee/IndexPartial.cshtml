@model IList<Fy3y.Model.Das.Employee>
<ul class="nav nav-tabs" id="mytab" role="tablist">
  <li class="nav-item">
    <a class="active nav-link" data-toggle="tab" href="#A">在职</a>
  </li>
  <li class="nav-item">
    <a class="nav-link" data-toggle="tab" href="#B">其他</a>
  </li>
  <li class="nav-item">
    <a class="btn btn-dark" href="@Url.Action("Create")">
      <i class="fa fa-plus">添加</i>
    </a>
  </li>
</ul>
<div class="border border-top-0 p-2 tab-content">
    <div class="active fade show tab-pane" id="A">
        @Html.Partial("ListPartial", Model?.Where(p => p.StatusId == 1 && !p.IsVirtual).ToList())
       
    </div>
    <div class="fade tab-pane" id="B">
        @Html.Partial("ListPartial", Model?.Where(p => p.StatusId == 1 && p.IsVirtual).ToList())
    </div>
</div>